{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.php"
      }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://myn-note.vercel.app"
    },
    "outputDirectory": "public"
  }
  